{
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"  # Root domain content
    },
    {
      "source": "/games/wsopfreechips/(.*)",
      "destination": "/games/wsopfreechips/$1"  # Subdomain content
    }
  ],
  "redirects": [
    {
      "source": "/wsopfreechips/(.*)",
      "destination": "/games/wsopfreechips/$1",
      "permanent": true
    }
  ]
}