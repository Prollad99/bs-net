{
  "git": {
    "deploymentEnabled": {
      "main": false
    }
  },
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/wsopfreechips/(.*)",
      "destination": "/games/wsopfreechips/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/games/wsopfreechips/$1",
      "has": [
        {
          "type": "host",
          "value": "wsopfreechips.bonuscollector.net"
        }
      ]
    },
    {
      "source": "/",
      "destination": "/games/wsopfreechips/index.html",
      "has": [
        {
          "type": "host",
          "value": "wsopfreechips.bonuscollector.net"
        }
      ]
    }
  ]
}