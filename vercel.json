{
  "version": 2,
  "builds": [
    {
      "src": "Gemfile",
      "use": "@vercel/ruby"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "^/wsopfreechips/(.*)",
      "dest": "/_site/wsopfreechips/$1"
    },
    {
      "src": "^/hititrichfreecoins/(.*)",
      "dest": "/_site/hititrichfreecoins/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "scripts": {
    "build": "jekyll build"
  }
}